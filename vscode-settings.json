{
    "editor.lineNumbers": "relative",
    "workbench.colorTheme": "Default Dark Modern",
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false,
    "editor.cursorSurroundingLines": 12,
    "java.import.gradle.java.home": "/Library/Java/JavaVirtualMachines/jdk-16.0.2.jdk/Contents/Home",
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [" ", "n", "e"],
            "after": [],
            "commands": [
                { "command": "editor.action.marker.next", "args":[]}
            ]
        },
        {
            "before": [" ", "p", "e"],
            "after": [],
            "commands": [
                { "command": "editor.action.marker.prev", "args":[]}
            ]
        },
        {
            "before": ["K"],
            "after": [],
            "commands": [
                { "command": "editor.action.showHover", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "l"],
            "after": [],
            "commands": [
                { "command": "eslint.executeAutofix", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "o"],
            "after": [],
            "commands": [
                { "command": "editor.action.formatDocument", "args":[]}
            ]
        },
        {
            "before": [" ", "w", "L"],
            "after": [],
            "commands": [
                { "command": "workbench.action.moveEditorToNextGroup", "args":[]}
            ]
        },
        {
            "before": [" ", "w", "H"],
            "after": [],
            "commands": [
                { "command": "workbench.action.moveEditorToLeftGroup", "args":[]}
            ]
        },
        {
            "before": ["g", "s"],
            "after": [],
            "commands": [
                { "command": "editor.action.revealDefinitionAside", "args":[]}
            ]
        },
        {
            "before": [" ", "c", "o"],
            "after": [],
            "commands": [
                { "command": "editor.action.commentLine", "args":[]}
            ]
        },
        {
            "before": ["g", "u"],
            "after": [],
            "commands": [
                { "command": "editor.action.goToReferences", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "f"],
            "after": [],
            "commands": [
                { "command": "workbench.action.quickOpen", "args":[]}
            ]
        }
    ],
    "vim.commandLineModeKeyBindings": [],
    "vim.vimrc.path": "$HOME/.vimrc",
    "vim.vimrc.enable": true,
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "**/*.tex"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.zoomLevel": 1,
    "editor.inlineSuggest.enabled": true,
    "files.hotExit": "off",
    "terminal.integrated.showExitAlert": true,
    "terminal.integrated.confirmOnExit": "always",
    "[python]": {
        "editor.formatOnType": true
    }
}