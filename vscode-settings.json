{
    "editor.lineNumbers": "relative",
    "editor.rulers": [90],
    "workbench.colorTheme": "Default Dark Modern",
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false,
    "editor.cursorSurroundingLines": 12,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [" ", "n", "e"],
            "after": [],
            "commands": [
                { "command": "editor.action.marker.next", "args":[]}
            ]
        },
        {
            "before": [" ", "p", "e"],
            "after": [],
            "commands": [
                { "command": "editor.action.marker.prev", "args":[]}
            ]
        },
        {
            "before": ["K"],
            "after": [],
            "commands": [
                { "command": "editor.action.showHover", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "l"],
            "after": [],
            "commands": [
                { "command": "eslint.executeAutofix", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "o"],
            "after": [],
            "commands": [
                { "command": "editor.action.formatDocument", "args":[]}
            ]
        },
        {
            "before": [" ", "w", "L"],
            "after": [],
            "commands": [
                { "command": "workbench.action.moveEditorToNextGroup", "args":[]}
            ]
        },
        {
            "before": [" ", "w", "H"],
            "after": [],
            "commands": [
                { "command": "workbench.action.moveEditorToLeftGroup", "args":[]}
            ]
        },
        {
            "before": ["g", "s"],
            "after": [],
            "commands": [
                { "command": "editor.action.revealDefinitionAside", "args":[]}
            ]
        },
        {
            "before": [" ", "c", "o"],
            "after": [],
            "commands": [
                { "command": "editor.action.commentLine", "args":[]}
            ]
        },
        {
            "before": ["g", "u"],
            "after": [],
            "commands": [
                { "command": "editor.action.goToReferences", "args":[]}
            ]
        },
        {
            "before": [" ", "f", "f"],
            "after": [],
            "commands": [
                { "command": "workbench.action.quickOpen", "args":[]}
            ]
        },
        {
            "before": [" ", "e", "p"],
            "after": [],
            "commands": [
                { "command": "workbench.action.toggleSidebarVisibility", "args":[]}
            ]
        },
        {
            "before": [" ", "r"],
            "after": [],
            "commands": [
                { "command": "editor.action.rename", "args":[]}
            ]
        }
    ],
    "vim.commandLineModeKeyBindings": [],
    "vim.vimrc.path": "$HOME/.vimrc",
    "vim.vimrc.enable": true,
    "vim.foldfix": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.inlineSuggest.enabled": true,
    "files.hotExit": "off",
    "terminal.integrated.showExitAlert": true,
    "terminal.integrated.confirmOnExit": "always",
    "redhat.telemetry.enabled": true,
    "yaml.format.singleQuote": true
}